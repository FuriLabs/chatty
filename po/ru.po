msgid "Show release version"
msgstr "Показать версию выпуска"

#: src/chatty-application.c:81
msgid "Start in daemon mode"
msgstr "Запустить в режиме службы"

#: src/chatty-application.c:82
msgid "Disable all accounts"
msgstr "Отключить все уч. записи"

#: src/chatty-application.c:84
msgid "Enable libpurple debug messages"
msgstr "Включить сообщения отладки libpurple"

#: src/chatty-application.c:87
msgid "Enable verbose debug messages (repeat option for more verbosity)"
msgstr ""
"Включить подробные сообщения отладки (опция повтора для большей детализации)"

