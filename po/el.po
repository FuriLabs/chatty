msgid "Chatty"
msgstr "Chatty"

#: data/sm.puri.Chatty.appdata.xml.in:7
msgid "A messaging application"
msgstr "Εφαρμογή μηνυμάτων"

#: data/sm.puri.Chatty.appdata.xml.in:8
msgid "Chatty is a messaging application supporting XMPP and SMS."
msgstr "Το Chatty είναι μια εφαρμογή μηνυμάτων μέσω XMPP και SMS."

#: data/sm.puri.Chatty.appdata.xml.in:16
msgid "Chatty's message window"
msgstr "Παράθυρο συνομιλιών του Chatty"

#: data/sm.puri.Chatty.desktop.in:5
msgid "sm.puri.Chatty"
msgstr "sm.puri.Chatty"

#: data/sm.puri.Chatty.desktop.in:6
msgid "SMS and XMPP chat application"
msgstr "Εφαρμογή μυνημάτων SMS και XMPP"

#: data/sm.puri.Chatty.desktop.in:7
msgid "XMPP;SMS;chat;jabber;messaging;modem"
msgstr "XMPP;SMS;chat;jabber;messaging;modem"

#: src/chatty-account.c:169
msgid "SMS Number"
msgstr "Αριθμός SMS"

#: src/chatty-account.c:171
msgid "Jabber ID"
msgstr "Jabber ID"

#: src/chatty-account.c:193
msgid "Delete Account"
msgstr "Διαγραφή Λογαριασμού"

#: src/chatty-account.c:196
#, c-format
msgid "Delete account %s?"
msgstr "Να διαγραφεί ο λογαριασμός %s?"

#: src/chatty-account.c:281
msgid "connected"
msgstr "σύνδεση επιτυχής"

#: src/chatty-account.c:283
msgid "connecting..."
msgstr "σύνδεση σε εξέλιξη..."

#: src/chatty-account.c:285
msgid "disconnected"
msgstr "η σύνδεση διακόπηκε"

#: src/chatty-account.c:496
msgid "Add new account..."
msgstr "Προσθήκη νέου λογαριασμού..."

#: src/chatty-account.c:523
msgid "Contact added"
msgstr "Προσθήκη επαφής επιτυχής"

#: src/chatty-account.c:527
#, c-format
msgid "User %s has added %s to the contacts"
msgstr "Ο χρήστης %s πρόσθεσε τον/την %s στις επαφές"

#: src/chatty-account.c:588
#, c-format
msgid "Authorize %s?"
msgstr "Εξουσιοδώτηση %s?"

#: src/chatty-account.c:592
msgid "Reject"
msgstr "Απόρριψη"

#: src/chatty-account.c:594
msgid "Accept"
msgstr "Αποδοχή"

#: src/chatty-account.c:599
#, c-format
msgid "Add %s to contact list"
msgstr "Προσθήκη %s στην λίστα επαφών"

#: src/chatty-buddy-list.c:600
#, c-format
msgid "Delete Chat with %s?"
msgstr "Να διαγραφεί η συνομιλία με %s?"

#: src/chatty-buddy-list.c:604
msgid "Cancel"
msgstr "Ακύρωση"

#: src/chatty-buddy-list.c:606
msgid "Delete"
msgstr "Διαγραφή"

#: src/chatty-buddy-list.c:611
msgid "This deletes the conversation history"
msgstr "Αυτή η ενέργεια θα διαγράψει το αρχείο συνομιλιών"

#: src/chatty-buddy-list.c:1563 src/chatty-buddy-list.c:1687
msgid "Me: "
msgstr "Εγώ:"

#: src/chatty-message-list.c:68 src/chatty-message-list.c:73
msgid "This is a IM conversation."
msgstr "Συνομιλία άμεσων μηνυμάτων (IM)."

#: src/chatty-message-list.c:69 src/chatty-message-list.c:79
msgid "Your messages are not encrypted,"
msgstr "Τα μηνύματά σας δεν τυγχάνουν κρυπτογράφησης,"

#: src/chatty-message-list.c:70
msgid "ask your counterpart to use E2EE."
msgstr "ζητήστε από το συνομιλητή σας να ενεργοποιήσει την κρυπτογράφηση."

#: src/chatty-message-list.c:74
msgid "Your messages are secured"
msgstr "Τα μηνύματά σας είναι ασφαλισμένα"

#: src/chatty-message-list.c:75
msgid "by end-to-end encryption."
msgstr "με κρυπτογράφηση end-to-end."

#: src/chatty-message-list.c:78
msgid "This is a SMS conversation."
msgstr "Αυτή είναι μια συνομιλία SMS."

#: src/chatty-message-list.c:80
msgid "and carrier rates may apply."
msgstr "και οι συνήθεις χρεώσεις παρόχου πιθανόν να υφίστανται."

#: src/chatty-window.c:78
msgid "No account selected"
msgstr "Δεν επελέγη λογαριασμός συνομιλιών"

#: src/chatty-window.c:81
#, c-format
msgid "Please select a chat account for\n"
"contact %s"
msgstr "Επιλέξτε λογαριασμό συνομιλιών για\n"
"την επαφή %s"
