---
 src/chatty-clock.c | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/src/chatty-clock.c b/src/chatty-clock.c
index be29ad9..ba69963 100644
--- a/src/chatty-clock.c
+++ b/src/chatty-clock.c
@@ -237,7 +237,7 @@ sync_time_source (ChattyClock *self)
 static void
 chatty_clock_finalize (GObject *object)
 {
